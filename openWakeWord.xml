<?xml version="1.0"?>
<Container version="2">
  <Name>openWakeWord</Name>
  <Repository>rhasspy/wyoming-openwakeword</Repository>
  <Registry>https://hub.docker.com/r/rhasspy/wyoming-openwakeword/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/72033-support-digiblurs-docker-template-repository/</Support>
  <Project>https://github.com/rhasspy/wyoming-openwakeword</Project>
  <Overview>Wyoming protocol server for openWakeWord wake word detection system.  There is NO WEB-UI to this container.</Overview>
  <Category>HomeAutomation: Status:Stable</Category>
  <TemplateURL>https://raw.githubusercontent.com/digiblur/unraid-docker-templates/master/openWakeWord.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/digiblur/unraid-docker-templates/master/images/openwakeword.png</Icon>
  <PostArgs>--preload-model 'ok_nabu'</PostArgs>
  <DonateText>Help support my work - buy me a drank</DonateText>
  <DonateLink>paypal.me/digiblurDIY</DonateLink>
  <Config Name="WyomingPort" Target="10400" Default="10400" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"/>
</Container>
